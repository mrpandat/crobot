{
  "name": "crobot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "eslint src",
    "start": "babel src -d lib && node lib/index.js",
    "prettier":
      "(git diff --name-only -- '*.js' && git diff --cached --name-only -- '*.js') | xargs yarn prettier-write",
    "prettier-check":
      "prettier --list-different --single-quote --trailing-comma es5 --parser babylon",
    "prettier-debug":
      "prettier --debug-check --single-quote --trailing-comma es5 --parser babylon",
    "prettier-write":
      "prettier --write --single-quote --trailing-comma es5 --parser babylon",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["yarn prettier-check"]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@slack/client": "^3.10.0",
    "plural": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "eslint": "4.13.0",
    "eslint-plugin-flowtype": "2.39.1",
    "flow-bin": "0.61.0",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "prettier": "1.9.1"
  }
}
